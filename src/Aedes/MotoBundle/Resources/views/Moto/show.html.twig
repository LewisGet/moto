{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Moto</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Title</th>
                <td>{{ entity.title }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ entity.price }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ entity.status }}</td>
            </tr>
            <tr>
                <th>Displacement</th>
                <td>{{ entity.displacement }}</td>
            </tr>
            <tr>
                <th>Fuel</th>
                <td>{{ entity.fuel }}</td>
            </tr>
            <tr>
                <th>Years</th>
                <td>{{ entity.years }}</td>
            </tr>
            <tr>
                <th>Mileage</th>
                <td>{{ entity.mileage }}</td>
            </tr>
            <tr>
                <th>Cooling</th>
                <td>{{ entity.cooling }}</td>
            </tr>
            <tr>
                <th>Modifiedcontent</th>
                <td>{{ entity.modifiedContent }}</td>
            </tr>
            <tr>
                <th>Tradingcontent</th>
                <td>{{ entity.tradingContent }}</td>
            </tr>
            <tr>
                <th>Brand</th>
                <td>{{ entity.brand }}</td>
            </tr>
            <tr>
                <th>Image</th>
				<td>
					{% for image in entity.image %}
						{{ image }}
					{% endfor %}
				</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('moto') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('moto_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
